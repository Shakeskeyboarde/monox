{
  "private": true,
  "name": "test-vite-lib-bundle",
  "description": "Testing the build command vite library mode with bundling",
  "version": "",
  "scripts": {},
  "type": "module",
  "types": "./lib/esm/bundle.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/cjs/bundle.d.ts",
        "default": "./lib/cjs/bundle.cjs"
      },
      "import": {
        "types": "./lib/esm/bundle.d.ts",
        "default": "./lib/esm/bundle.js"
      }
    },
    "./sub": {
      "require": {
        "types": "./lib/cjs/foo.d.ts",
        "default": "./lib/cjs/foo.cjs"
      },
      "import": {
        "types": "./lib/esm/foo.d.ts",
        "default": "./lib/esm/foo.js"
      }
    },
    "./svg": {
      "require": {
        "types": "./lib/cjs/test.d.ts",
        "default": "./lib/cjs/test.cjs"
      },
      "import": {
        "types": "./lib/esm/test.d.ts",
        "default": "./lib/esm/test.js"
      }
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "@wurk/command-build": "*",
    "vite": "^5.1.1",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-svgr": "^4.2.0"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.133"
  }
}
