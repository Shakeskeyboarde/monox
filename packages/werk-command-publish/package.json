{
  "name": "@werk/command-publish",
  "description": "Publish workspaces.",
  "version": "1.2.2",
  "author": "Chris Ackerman <chris@topher.land>",
  "license": "ISC",
  "homepage": "https://github.com/Shakeskeyboarde/werk/blob/main/packages/werk-command-publish/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/werk.git",
    "directory": "packages/werk-command-publish"
  },
  "type": "module",
  "exports": {
    "types": "./lib/main.d.ts",
    "default": "./lib/main.js"
  },
  "files": [
    "CHANGELOG.md",
    "lib/**"
  ],
  "keywords": [
    "werk",
    "werk-command",
    "monorepo",
    "workspaces",
    "npm",
    "command",
    "publish",
    "deploy",
    "release",
    "registry"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/tar-stream": "^2.2.2",
    "@werk/cli": "^1.2.1",
    "tar-stream": "^3.1.3"
  },
  "werk": {
    "tsc": {
      "config": [
        {
          "compilerOptions": {
            "rootDir": "src",
            "outDir": "lib",
            "noEmit": false
          },
          "include": [
            "src"
          ],
          "exclude": [
            "**/*.test.*"
          ]
        }
      ]
    }
  }
}
