{
  "private": true,
  "name": "test-vite-lib",
  "description": "Testing the build command vite library mode",
  "type": "module",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.cjs"
      },
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      }
    },
    "./component": {
      "require": {
        "types": "./lib/cjs/foo/component.d.ts",
        "default": "./lib/cjs/foo/component.cjs"
      },
      "import": {
        "types": "./lib/esm/foo/component.d.ts",
        "default": "./lib/esm/foo/component.js"
      }
    }
  },
  "dependencies": {
    "@types/react": "^18.2.42",
    "react": "^18.2.0",
    "test-vite-lib-bundle": "*"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "@werk/command-build": "*",
    "vite": "^5.0.7",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-plugin-zip-pack": "^1.0.6"
  }
}
