{
  "private": true,
  "name": "test-vite-lib",
  "description": "Testing the build command vite library mode",
  "type": "module",
  "types": "./lib/index.d.ts",
  "main": "./lib/index.cjs",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "default": "./lib/index.js"
    },
    "./component": {
      "types": "./lib/component.d.ts",
      "require": "./lib/component.cjs",
      "default": "./lib/component.js"
    }
  },
  "dependencies": {
    "@types/react": "^18.2.38",
    "react": "^18.2.0",
    "test-vite-lib-bundle": "*"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.2",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-svgr": "^4.2.0"
  }
}
