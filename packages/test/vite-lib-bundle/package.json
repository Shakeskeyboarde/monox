{
  "private": true,
  "name": "test-vite-lib-bundle",
  "description": "Testing the build command vite library mode with bundling",
  "type": "module",
  "types": "./lib/bundle.d.ts",
  "main": "./lib/bundle.cjs",
  "exports": {
    ".": {
      "types": "./lib/bundle.d.ts",
      "require": "./lib/bundle.cjs",
      "default": "./lib/bundle.js"
    },
    "./sub": {
      "types": "./lib/foo.d.ts",
      "require": "./lib/foo.cjs",
      "default": "./lib/foo.js"
    },
    "./svg": {
      "types": "./lib/test.d.ts",
      "require": "./lib/test.cjs",
      "default": "./lib/test.js"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.2",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-svgr": "^4.2.0"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.129"
  }
}
