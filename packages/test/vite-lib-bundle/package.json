{
  "private": true,
  "name": "test-vite-bundle",
  "description": "Testing the build command vite library mode with bundling",
  "type": "module",
  "types": "./lib/bundle.d.ts",
  "main": "./lib/bundle.cjs",
  "exports": {
    ".": {
      "types": "./lib/bundle.d.ts",
      "require": "./lib/bundle.cjs",
      "default": "./lib/bundle.js"
    },
    "./sub": {
      "types": "./lib/foo.d.ts",
      "require": "./lib/foo.cjs",
      "default": "./lib/foo.js"
    },
    "./svg": {
      "types": "./lib/test.d.ts",
      "require": "./lib/test.cjs",
      "default": "./lib/test.js"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.4",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-svgr": "^4.0.0"
  }
}
