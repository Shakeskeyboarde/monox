{
  "private": true,
  "name": "root",
  "author": "Chris Ackerman <chris@topher.land>",
  "version": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/wurk.git"
  },
  "type": "module",
  "scripts": {
    "clean": "git clean -fdX -e '!/.*' -e '!**/node_modules' -e '!**/node_modules/**'",
    "clean:all": "git clean -fdX -e '!/.*'",
    "eslint": "wurk exec eslint src",
    "depcheck": "wurk exec depcheck --quiet --skip-missing",
    "build-scripts": "pnpm -r build",
    "build": "pnpm build-scripts && wurk run build --stream",
    "test": "pnpm build && wurk eslint -- --max-warnings=0 && wurk depcheck && wurk vitest run",
    "modernize": "pnpm dlx npm-check-updates@latest -i --workspaces --root --format group"
  },
  "packageManager": "pnpm@9.1.1+sha256.9551e803dcb7a1839fdf5416153a844060c7bce013218ce823410532504ac10b",
  "devDependencies": {
    "@types/node": "^20.12.11",
    "@vitest/coverage-v8": "^1.6.0",
    "@wurk/command-exec": "workspace:^",
    "@wurk/command-list": "workspace:^",
    "@wurk/command-publish": "workspace:^",
    "@wurk/command-run": "workspace:^",
    "@wurk/command-version": "workspace:^",
    "@wurk/command-vitest": "workspace:^",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-rational": "^5.0.14",
    "rollup": "^4.17.2",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^4.0.1",
    "typescript": "5.4.x",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "wurk": "workspace:^"
  }
}
